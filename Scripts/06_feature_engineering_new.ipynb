    "def get_temporal_features(data):\n",
    "    date = data['date']\n",
    "    data[\"year\"] = date.dt.year\n",
    "    data[\"season\"] = date.dt.quarter\n",
    "    data[\"month\"] = date.dt.month\n",
    "\n",
    "    return data\n",
    "\n",
    "t = get_temporal_features(df)\n",
    "t[t[\"season\"] == 3]"
